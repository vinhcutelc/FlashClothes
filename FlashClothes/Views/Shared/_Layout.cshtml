<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Flash Clothes</title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
    <!-- Bootstrap core CSS -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="~/Content/css/mdb.min.css" rel="stylesheet">
    <!-- Your custom styles (optional) -->
    <link href="~/Content/css/style.min.css" rel="stylesheet">
    <link rel="icon" href="~/Areas/Admin/Asset/images/favicon.ico" type="image/x-icon">
    <script type="text/javascript" src="~/scripts/jquery-3.4.1.min.js"></script>
    <style type="text/css">
        /* Required height of parents of the Half Page Carousel for proper displaying carousel itself */
        html,
        body,
        .view {
            height: 100%;
        }

        /* Half Page Carousel itself*/
        .carousel {
            height: 50%;
        }

            .carousel .carousel-inner {
                height: 100%;
            }

                .carousel .carousel-inner .carousel-item,
                .carousel .carousel-inner .active {
                    height: 100%;
                }

        /* Adjustment for mobile devices*/

        /* Footer color for sake of consistency with Navbar */
        .page-footer {
            background-color: #929FBA;
        }
    </style>
</head>

<body>

    <!-- Navbar -->
    <nav class="navbar fixed-top navbar-expand-lg navbar-light white scrolling-navbar">
        <div class="container">

            <!-- Brand -->
            <a class="navbar-brand waves-effect" href="@Url.Action("Index","Home")">
                <strong class="blue-text">Flash Clothes</strong>
            </a>

            <!-- Collapse -->
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <!-- Links -->
            <div class="collapse navbar-collapse" id="navbarSupportedContent">

                <!-- Left -->
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <a class="nav-link waves-effect" href="@Url.Action("Index","Home")">
                            Trang chủ
                            <span class="sr-only">(current)</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link waves-effect" href="@Url.Action("About","Home")" target="_blank">Thông Tin Shop</a>
                    </li>
                    @if (HttpContext.Current.Session["Name"] != null)
                    {
                        <li class="nav-item">
                            <a class="nav-link waves-effect" href="@Url.Action("DetailOrder","Order")" target="_blank">Đơn Hàng</a>
                        </li>
                    }
                </ul>

                <!-- Right -->
                <ul class="navbar-nav nav-flex-icons">
                    @{ Html.RenderAction("CartItem", "Home");}
                    <li class="nav-item d-flex">
                        @if (HttpContext.Current.Session["Name"] == null)
                        {
                            <a href="@Url.Action("Index","Login")" class="nav-link border border-light rounded waves-effect">
                                <i class="fa fa-user"></i><span style="margin:0px 10px">Đăng nhập</span>
                            </a>
                        }
                        else
                        {
                            <a href="" class="nav-link border border-light rounded waves-effect"
                               target="_blank">
                                <i class="fa fa-user"></i><span style="margin:0px 10px">@HttpContext.Current.Session["Name"]</span>
                            </a>
                            using (Html.BeginForm("LogOut", "Login", FormMethod.Post))
                            {
                                <button class="btn btn-primary btn-md my-0 p" type="submit"><i class="fas fa-sign-out-alt ml-1"></i></button>
                            }
                        }
                    </li>
                </ul>

            </div>

        </div>
    </nav>
